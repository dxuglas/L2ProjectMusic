QFrame#MediaInterface {
  background-color: black;
}


QSlider::handle:horizontal#bar {
  background: transparent;
  width: 12;
  margin: -4px 0;
}

QSlider::handle:horizontal:hover#bar {
  background: white;
  width: 12;
  margin: -4px 0;
}

QSlider::groove:horizontal#bar {
  background: transparent;
}

QSlider::add-page:horizontal#bar {
  background: rgb(77, 77, 77);
}

QSlider::sub-page:horizontal#bar {
  background: white;
}